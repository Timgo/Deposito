<div class="depot-form">
<h1>Log In Administrativo</h1>

<% form_tag do %>

  <p>
    <label for="nome">Nome:</label>
    <%= text_field_tag :nome, params[:nome] %>
  </p>
  <p>
    <label for="senha">Senha:</label>
    <%= password_field_tag :senha, params[:senha] %>
  </p>
  <p>
    <%= submit_tag "Login" %>
  </p>
<% end %>
</div>
